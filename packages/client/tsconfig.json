{
  "extends": "../../tsconfig.json",
  "include": ["./src/**/*.ts"],
  "noEmit": true,
  "compilerOptions": {
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "composite": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "es6",
    "plugins": [{ "name": "typescript-plugin-css-modules" }]
  },
  "references": [
    {
      "path": "../server"
    }
  ]
}

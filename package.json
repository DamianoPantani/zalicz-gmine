{
  "name": "zalicz-gmine-monorepo",
  "version": "1.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:client": "npm run start --workspace=packages/client",
    "start:server": "npm run start --workspace=packages/server",
    "build:server": "npm run build --workspace=packages/server",
    "build:server:voivodships": "npm run compileVoivodships --workspace=packages/server",
    "build:server:mapCoords": "npm run compileMapCoords --workspace=packages/server",
    "build:server:capitalCoords": "npm run compileCapitalCoords --workspace=packages/server",
    "lint": "eslint --ignore-path .gitignore packages/*/src/**/*.{ts,tsx}",
    "format": "prettier --write --ignore-path .gitignore packages/*/src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "autoprefixer": "10.4.5"
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "typescript": "4.7.4"
  },
  "resolutions": {
    "typescript": "4.7.4",
    "tslib": "2.4.0"
  }
}
